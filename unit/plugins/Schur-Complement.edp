macro FindDofOnBorder(l,Vh,I)
{
	varf von(u,v) = on(l,u=-1);
    int[int] J=0:Vh.ndof-1;
	real[int] vl = von(0,Vh);
	sort(vl,J);
	vl = vl ? 1. :0.;
	int n = vl.sum+0.5;

	I.resize(n);
	I=J(0:n-1);
	cout << " n = "<< n << endl;
}//

macro grad(u) [dx(u),dy(u)] //
real hsize= 0.1;

