 // routine
func int fi(real  a1)
{
   int i=a1*a1/1;
   return i;
}; 
func real myfunction(real a,real b)
{
   real aa=a;
   real z =fi(aa)+b;
   return z;
};
// appele 
cout << myfunction(1.0,3.) << " " << myfunction(1,3)  << endl;

// Regression check
real ref=myfunction(1.0,3.);
cout<<"reference value="<<ref<<endl;
assert(ref<4.000001);
assert(ref>3.999999);
