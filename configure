          ppc*) # G3 ????
	       ff_fast="-O3";;
	  i486)
	    ff_fast="-O3 -march=pentium4";;
	  *)
	    as_fn_error "cannot determine apple cpu type " "$LINENO" 5
	    ff_fast="-O3";;
	 esac


