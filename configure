else
  ff_suffix="-$with_suffix"
  { echo "$as_me:$LINENO: result: $ff_suffix" >&5
echo "${ECHO_T}$ff_suffix" >&6; }
fi
ADD_PACKAGE_NAME=$ff_suffix


