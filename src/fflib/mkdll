echo g++ -mno-cygwin -shared  $OBJ1 libff0.dll  -o libff1.dll

g++  -shared  -mno-cygwin -mwindows -mthreads  $OBJ2 $LDADD2 -o libff2.dll 
g++  ../std/Pcrgraph.o -mno-cygwin -mwindows -mthreads libff?.dll -o FreeFem++.exe 
g++  ../nw/sansrgraph.o -mno-cygwin libff?.dll -o FreeFem++-nw.exe   

echo *.dll *.exe